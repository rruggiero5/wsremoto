package br.com.unixyz.beans;

public class Endereco {

	private String logradouro;
	private String numero;
	private boolean complemento;
	private String bairro;
	private String uf;
	private String cep;
	
		
	
	
	
	public Endereco() {
		super();
	}
	public Endereco(String logradouro, String numero, boolean complemento, String bairro, String uf, String cep) {
		super();
		this.logradouro = logradouro;
		this.numero = numero;
		this.complemento = complemento;
		this.bairro = bairro;
		this.uf = uf;
		this.cep = cep;
	}
	
	
	public String getLogradouro() {
		return logradouro;
	}
	public void setLogradouro(String logradouro) {
		this.logradouro = logradouro;
	}
	public String getNumero() {
		return numero;
	}
	public void setNumero(String numero) {
		this.numero = numero;
	}
	public boolean isComplemento() {
		return complemento;
	}
	public void setComplemento(boolean complemento) {
		this.complemento = complemento;
	}
	public String getBairro() {
		return bairro;
	}
	public void setBairro(String bairro) {
		this.bairro = bairro;
	}
	public String getUf() {
		return uf;
	}
	public void setUf(String uf) {
		this.uf = uf;
	}
	public String getCep() {
		return cep;
	}
	public void setCep(String cep) {
		this.cep = cep;
	}
	
	
	
	
	
	
	
}
